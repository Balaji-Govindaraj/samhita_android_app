        <activity
            android:name="com.example.samhita.Online_List_Main"
            android:label="@string/online" >
        </activity>
        <activity
            android:name="com.example.samhita.General_List_Main"
            android:label="@string/general" >
        </activity>
        <activity
            android:name="com.example.samhita.Contact_List_Main"
            android:label="@string/contact" >
        </activity>
        <activity
            android:name="com.example.samhita.Mega_Event"
            android:label="@string/mega_event" >
        </activity>
        <activity
            android:name="com.example.samhita.Welcome"
            android:label="@string/welcome" >
        </activity>
        <activity
            android:name="com.example.samhita.Create_Account"
            android:label="@string/welcome1" >
